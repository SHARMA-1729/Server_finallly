{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",  
      "use": "@vercel/node"        
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",  
      "dest": "/src/index.js"
       "methods": ["GET", "POST", "PATCH", "DELETE", "OPTIONS"]
    },
    {
      "src": "/(.*)",  
      "dest": "/src/app.js"
    }
  ]
}
